<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".serviceScheduler">

    <Button
        android:id="@+id/oilChange6"
        android:layout_width="227dp"
        android:layout_height="0dp"
        android:text="Alignment"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/oilChange4" />

    <Button
        android:id="@+id/oilChange3"
        android:layout_width="227dp"
        android:layout_height="0dp"
        android:layout_marginBottom="24dp"
        android:text="Spark Plugs"
        app:layout_constraintBottom_toTopOf="@+id/oilChange2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/oilChange5" />

    <Button
        android:id="@+id/oilChange4"
        android:layout_width="227dp"
        android:layout_height="0dp"
        android:layout_marginBottom="20dp"
        android:text="Tire Rotation"
        app:layout_constraintBottom_toTopOf="@+id/oilChange6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/oilChange2" />

    <Button
        android:id="@+id/oilChange5"
        android:layout_width="227dp"
        android:layout_height="0dp"
        android:layout_marginBottom="20dp"
        android:text="Brakes"
        app:layout_constraintBottom_toTopOf="@+id/oilChange3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/oilChange" />

    <Button
        android:id="@+id/oilChange2"
        android:layout_width="227dp"
        android:layout_height="0dp"
        android:layout_marginBottom="25dp"
        android:text="Tire Change"
        app:layout_constraintBottom_toTopOf="@+id/oilChange4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/oilChange3" />

    <Button
        android:id="@+id/oilChange"
        android:layout_width="227dp"
        android:layout_height="0dp"
        android:layout_marginTop="237dp"
        android:layout_marginBottom="12dp"
        android:text="Oil Change"
        app:layout_constraintBottom_toTopOf="@+id/oilChange5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="189dp"
        android:layout_height="381dp"
        android:layout_marginBottom="76dp"
        app:layout_constraintBottom_toBottomOf="@+id/oilChange5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/carminder_logo" />


</androidx.constraintlayout.widget.ConstraintLayout>